{
  "name": "@my-workspace/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "// Backend": "",
    "start:api": "nx serve url-shortner",
    "start:api:dev": "nx serve url-shortner --configuration=development",
    "start:api:prod": "nx serve url-shortner --configuration=production",
    "build:api": "nx build url-shortner",
    "build:api:prod": "nx build url-shortner --configuration=production",
    "test:api": "nx test url-shortner",
    "test:api:watch": "nx test url-shortner --watch",
    "test:api:coverage": "nx test url-shortner --coverage",
    "e2e:api": "nx e2e url-shortner-e2e",
    "swagger": "nx serve url-shortner & open http://localhost:3000/api/docs",
    "// Frontend": "",
    "start:web": "nx serve web",
    "start:web:dev": "nx serve web --configuration=development",
    "start:web:prod": "nx serve web --configuration=production",
    "build:web": "nx build web",
    "build:web:prod": "nx build web --configuration=production",
    "test:web": "nx test web",
    "test:web:watch": "nx test web --watch",
    "test:web:coverage": "nx test web --coverage",
    "e2e:web": "nx e2e web-e2e",
    "storybook:web": "nx storybook web",
    "build-storybook:web": "nx build-storybook web",
    "// Common": "",
    "start": "concurrently -n \"API,FRONTEND\" -c \"bgBlue.bold,bgGreen.bold\" \"nx serve url-shortner\" \"nx serve web\"",
    "start:dev": "concurrently -n \"API,FRONTEND\" -c \"bgBlue.bold,bgGreen.bold\" \"nx serve url-shortner --configuration=development\" \"nx serve web --configuration=development\"",
    "build": "nx run-many --target=build --all",
    "build:prod": "nx run-many --target=build --all --configuration=production",
    "test": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all --watch",
    "test:coverage": "nx run-many --target=test --all --coverage",
    "lint": "nx run-many --target=lint --all",
    "format": "prettier --write \"**/*.{ts,tsx,json,md,html,css,scss}\""
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/swagger": "^11.2.0",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "mongoose": "^8.14.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/devkit": "21.0.3",
    "@nx/eslint": "21.0.3",
    "@nx/eslint-plugin": "21.0.3",
    "@nx/jest": "21.0.3",
    "@nx/js": "21.0.3",
    "@nx/nest": "21.0.3",
    "@nx/node": "21.0.3",
    "@nx/playwright": "21.0.3",
    "@nx/react": "^21.0.3",
    "@nx/vite": "21.0.3",
    "@nx/web": "21.0.3",
    "@nx/webpack": "21.0.3",
    "@nx/workspace": "21.0.3",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^3.0.0",
    "autoprefixer": "10.4.13",
    "concurrently": "^9.1.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "nx": "21.0.3",
    "postcss": "8.4.38",
    "prettier": "^2.6.2",
    "tailwindcss": "3.4.3",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.19.0",
    "vite": "^6.0.0",
